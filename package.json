{
  "name": "csv-backup",
  "version": "1.0.0",
  "description": "A set of scripts for downloading db tables for snapshot labs",
  "repository": "Todmy/csv-backup",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint --ext .ts . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node --transpile-only src/index.ts",
    "start:cron": "ts-node --transpile-only src/cron.ts",
    "dev": "nodemon src/index.ts",
    "dev:inspect": "nodemon --exec \"node --inspect-brk -r ts-node/register\" src/index.ts"
  },
  "eslintConfig": {
    "extends": "@snapshot-labs"
  },
  "prettier": "@snapshot-labs/prettier-config",
  "author": "Todmy",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.3.1",
    "cron": "^2.3.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "mysql2": "^3.4.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.0"
  },
  "devDependencies": {
    "@snapshot-labs/eslint-config": "^0.1.0-beta.7",
    "@snapshot-labs/prettier-config": "^0.1.0-beta.7",
    "@types/bluebird": "^3.5.38",
    "@types/cron": "^2.0.1",
    "eslint": "^8.43.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  }
}
